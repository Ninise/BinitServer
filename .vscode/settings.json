{
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.formatting.provider": "none",
  "files.watcherExclude": {
    "**/__pycache__**": true
  },
  "files.exclude": {
    "**/.vscode": true,
    "**/__pycache__": true
  },
  "search.exclude": {
    "**/__pycache__": true
  }
}
